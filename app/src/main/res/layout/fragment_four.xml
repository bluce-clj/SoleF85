<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="560dp"
    tools:context=".engineer.FourFragment">

    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="69dp"
        android:layout_marginBottom="68dp"
        android:fontFamily="@font/open_sans_semibold"
        android:text="Incline Calibration"
        android:textColor="@color/color597084"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/vv2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_cnt02"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="317dp"
        android:fontFamily="@font/hanzel_bold"
        android:gravity="center_vertical|end"
        android:letterSpacing="0.07"
        android:text="MAX. AD:"
        android:textAllCaps="true"
        android:textColor="@color/color597084"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@+id/vv1_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_marginStart="8dp"
        android:id="@+id/tv_max_ad_num"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:fontFamily="@font/hanzel_bold"
        android:gravity="center"
        android:letterSpacing="0.07"
        android:text=""
        android:textColor="@color/color597084"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_cnt02"
        app:layout_constraintStart_toEndOf="@+id/tv_cnt02"
        app:layout_constraintTop_toTopOf="@+id/tv_cnt02" />


    <TextView
        android:id="@+id/tv_cnt06"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="631dp"
        android:fontFamily="@font/hanzel_bold"
        android:gravity="center_vertical|end"
        android:letterSpacing="0.07"
        android:text="Direction:"
        android:textAllCaps="true"
        android:textColor="@color/color597084"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@+id/vv1_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_marginStart="8dp"
        android:id="@+id/tv_target_level_num"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:fontFamily="@font/hanzel_bold"
        android:gravity="center"
        android:letterSpacing="0.07"
        android:text=""
        android:textColor="@color/color597084"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_cnt06"
        app:layout_constraintStart_toEndOf="@+id/tv_cnt06"
        app:layout_constraintTop_toTopOf="@+id/tv_cnt06" />

    <View
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginStart="570dp"
        android:layout_marginTop="9dp"
        android:layout_marginBottom="9dp"
        android:background="@color/colorB4BEC7"
        app:layout_constraintBottom_toBottomOf="@+id/vv1_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_cnt04"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="317dp"
        android:fontFamily="@font/hanzel_bold"
        android:gravity="center_vertical|end"
        android:letterSpacing="0.07"
        android:text="Min. AD:"
        android:textAllCaps="true"
        android:textColor="@color/color597084"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@+id/vv2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/vv1_1" />

    <TextView
        android:layout_marginStart="8dp"
        android:id="@+id/tv_mid_ad_num"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:fontFamily="@font/hanzel_bold"
        android:gravity="center"
        android:letterSpacing="0.07"
        android:text=""
        android:textColor="@color/color597084"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@+id/vv2"
        app:layout_constraintStart_toEndOf="@+id/tv_cnt04"
        app:layout_constraintTop_toTopOf="@+id/vv1_1" />


    <View
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginStart="570dp"
        android:layout_marginTop="9dp"
        android:layout_marginBottom="9dp"
        android:background="@color/colorB4BEC7"
        app:layout_constraintBottom_toBottomOf="@+id/vv2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/vv1_1" />


    <TextView
        android:id="@+id/tv_cnt01"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="631dp"
        android:fontFamily="@font/hanzel_bold"
        android:gravity="center_vertical|end"
        android:letterSpacing="0.07"
        android:text="Current AD:"
        android:textAllCaps="true"
        android:textColor="@color/color597084"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@+id/vv2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/vv1_1" />


    <TextView
        android:id="@+id/tv_current_ad_num"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginEnd="385dp"
        android:fontFamily="@font/hanzel_bold"
        android:gravity="center"
        android:letterSpacing="0.07"
        android:text=""
        android:textColor="@color/color597084"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="@+id/vv2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tv_cnt01"
        app:layout_constraintTop_toTopOf="@+id/vv1_1" />

    <View
        android:id="@+id/vv1_1"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_marginStart="263dp"
        android:layout_marginEnd="287dp"
        android:background="@color/colorDEE2E6"
        app:layout_constraintBottom_toBottomOf="@+id/vv2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/btn_start"
        style="@style/NormalButton"
        android:layout_width="260dp"
        android:layout_height="64dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn_rrect_9d2227_288"
        android:text="START"
        android:textColor="@color/colorFFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/vv2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/vv2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="161dp"
        android:background="@color/colorDEE2E6"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:layout_marginStart="8dp"
        android:fontFamily="@font/hanzel_bold"
        android:letterSpacing="0.05"
        android:text="Level AD:"
        android:textAllCaps="true"
        android:textColor="@color/color597084"
        android:textSize="18sp"
        android:layout_marginBottom="200dp"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/level_ad"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginStart="8dp"
        android:fontFamily="@font/hanzel_bold"
        android:letterSpacing="0.05"
        android:text="Incline AD:"
        android:textAllCaps="true"
        android:textColor="@color/color597084"
        android:textSize="18sp"
        android:layout_marginBottom="100dp"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/incline_ad"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</androidx.constraintlayout.widget.ConstraintLayout>
