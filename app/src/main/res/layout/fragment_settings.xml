<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorFFFFFF"
    tools:context=".settings.SettingsFragment" >

    <TextView
        android:textAllCaps="true"
        android:textColor="@color/color597084"
        android:letterSpacing="0.14062"
        android:gravity="center"
        android:id="@+id/setting_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="104dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="40sp"
        android:fontFamily="@font/hanzel_bold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/settings_title" />

    <View
        android:background="@color/colorDEE2E6"
        android:id="@+id/ivTopLine_Programs"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="192dp"
        app:layout_constraintBottom_toTopOf="@+id/clListTemplates_Programs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        app:layout_constraintTop_toBottomOf="@+id/ivTopLine_Programs"
        android:id="@+id/clListTemplates_Programs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:visibility="visible">

        <Button
            android:fontFamily="@font/open_sans_semibold"
            android:textColor="@color/color597084"
            android:id="@+id/btSetting01"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="Display Brightness"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:layout_marginStart="24dp"
            android:fontFamily="@font/open_sans_semibold"
            android:textColor="@color/color597084"
            android:id="@+id/btSetting02"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/setting_wifi"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting01"
            app:layout_constraintTop_toTopOf="@+id/btSetting01" />


        <Button
            android:layout_marginStart="24dp"
            android:fontFamily="@font/open_sans_semibold"
            android:textColor="@color/color597084"
            android:id="@+id/btSetting03"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="Bluetooth"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting02"
            app:layout_constraintTop_toTopOf="@+id/btSetting02" />

        <Button
            android:layout_marginStart="24dp"
            android:fontFamily="@font/open_sans_semibold"
            android:textColor="@color/color597084"
            android:id="@+id/btSetting04"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/setting_software"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting03"
            app:layout_constraintTop_toTopOf="@+id/btSetting03" />

        <ImageView
            android:contentDescription="@string/img"
            android:visibility="invisible"
            android:id="@+id/iv_update_notify"
            android:layout_marginEnd="71dp"
            android:layout_marginTop="45dp"
            app:layout_constraintEnd_toEndOf="@+id/btSetting04"
            app:layout_constraintTop_toTopOf="@+id/btSetting04"
            android:src="@drawable/shape_red"
            android:layout_width="16dp"
            android:layout_height="16dp"/>

        <Button
            android:fontFamily="@font/open_sans_semibold"
            android:textColor="@color/color597084"
            android:id="@+id/btSetting05"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_marginStart="28dp"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/setting_date"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btSetting01" />

        <Button
            android:layout_marginStart="24dp"
            android:fontFamily="@font/open_sans_semibold"
            android:textColor="@color/color597084"
            android:id="@+id/btSetting06"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/setting_time"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting05"
            app:layout_constraintTop_toTopOf="@+id/btSetting05" />

        <Button
            android:layout_marginStart="24dp"
            android:fontFamily="@font/open_sans_semibold"
            android:textColor="@color/color597084"
            android:visibility="visible"
            android:id="@+id/btSetting07"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="Child Lock"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting06"
            app:layout_constraintTop_toTopOf="@+id/btSetting06" />

        <Button
            android:fontFamily="@font/open_sans_semibold"
            android:textColor="@color/color597084"
            app:layout_constraintEnd_toEndOf="parent"
            android:visibility="visible"
            android:id="@+id/btSetting08"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="Units"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting07"
            app:layout_constraintTop_toTopOf="@+id/btSetting07" />

        <Button
            android:layout_marginTop="24dp"
            android:textColor="@color/color597084"
            android:id="@+id/btSetting09"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="Display Mode"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="@+id/btSetting05"
            app:layout_constraintTop_toBottomOf="@+id/btSetting05" />

        <Button
            android:visibility="invisible"
            android:id="@+id/btSetting10"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/template_10"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting09"
            app:layout_constraintTop_toTopOf="@+id/btSetting09" />

        <Button
            android:visibility="invisible"
            android:id="@+id/btSetting11"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/template_11"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting10"
            app:layout_constraintTop_toTopOf="@+id/btSetting10" />

        <Button
            app:layout_constraintEnd_toEndOf="parent"
            android:visibility="invisible"
            android:id="@+id/btSetting12"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_rrect_ffffff_288"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/template_12"
            android:textAllCaps="false"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btSetting11"
            app:layout_constraintTop_toTopOf="@+id/btSetting11" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>