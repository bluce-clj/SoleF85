    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/title_text1"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="381dp"
        android:layout_marginEnd="380dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_workout_top_line"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:contentDescription="@string/img"
            android:id="@+id/iv_red"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/index_level_profile"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:textAllCaps="true"
            android:letterSpacing="0.1"
            android:id="@+id/tv_speed"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:fontFamily="@font/hanzel_bold"
            android:gravity="center_vertical"
            android:text="Level Profile"
            android:textColor="@color/colorFFFFFF"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/iv_orange"
            app:layout_constraintStart_toEndOf="@+id/iv_red"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:contentDescription="@string/img"
            android:id="@+id/iv_orange"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginStart="26dp"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/index_incline_profile"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tv_incline"
            app:layout_constraintStart_toEndOf="@+id/tv_speed"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:textAllCaps="true"
            android:letterSpacing="0.1"
            android:id="@+id/tv_incline"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:fontFamily="@font/hanzel_bold"
            android:gravity="start|center_vertical"
            android:text="Incline Profile"
            android:textColor="@color/colorFFFFFF"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iv_orange"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


